<data_types>
    <data_type>
        <id>file</id>
        <display_name>File</display_name>
        <processing_class>edu.cmu.pslc.learnsphere.imports.Import.File</processing_class>
        <filter_regex>.*</filter_regex>
        <data_type>
            <id>tab-delimited</id>
            <display_name>Tab-Delimited</display_name>
            <processing_class>edu.cmu.pslc.learnsphere.imports.Import.TabDelimited</processing_class>
            <data_type>
                <id>transaction</id>
                <display_name>Transaction</display_name>
                <processing_class>edu.cmu.pslc.learnsphere.imports.Import.TabDelimited</processing_class>
                <filter_regex>ds[0-9]+_tx_(.*)_[0-9]+_[0-9]+_[0-9]+_[0-9]+\.[a-z]+</filter_regex>
            </data_type>
            <data_type>
                <id>student-step</id>
                <display_name>Student-Step</display_name>
                <processing_class>edu.cmu.pslc.learnsphere.imports.Import.StudentStep</processing_class>
                <filter_regex>ds[0-9]+_student_step_(.*)_[0-9]+_[0-9]+_[0-9]+_[0-9]+\.[a-z]+</filter_regex>
            </data_type>
            <data_type>
                <id>student-problem</id>
                <display_name>Student-Problem</display_name>
                <processing_class>edu.cmu.pslc.learnsphere.imports.Import.TabDelimited</processing_class>
                <filter_regex>ds[0-9]+_student_problem_(.*)_[0-9]+_[0-9]+_[0-9]+_[0-9]+\.[a-z]+</filter_regex>
            </data_type>
            <data_type>
                <id>gradebook</id>
                <display_name>Gradebook</display_name>
                <processing_class>edu.cmu.pslc.learnsphere.imports.Import.TabDelimited</processing_class>
                <filter_regex>.*</filter_regex>
            </data_type>
        </data_type>
        <data_type>
            <id>xml</id>
            <display_name>XML</display_name>
            <processing_class>edu.cmu.pslc.learnsphere.imports.Import.Xml</processing_class>
            <filter_regex>.*</filter_regex>
        </data_type>
        <data_type>
            <id>csv</id>
            <display_name>CSV</display_name>
            <processing_class>edu.cmu.pslc.learnsphere.imports.Import.Csv</processing_class>
            <filter_regex>.*</filter_regex>
        </data_type>
        <data_type>
            <id>text</id>
            <display_name>Other Text</display_name>
            <processing_class>edu.cmu.pslc.learnsphere.imports.Import.Text</processing_class>
            <filter_regex>.*</filter_regex>
        </data_type>
    </data_type>
</data_types>